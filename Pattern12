class Solution {
public:
    void pattern12(int n) {

for(int m=1;m<=n;m++)
{
        for(int i=1;i<=m;i++)
        {
            cout<<i;
        }
        for(int sp=1;sp<=((2*n)-(2*m));sp++)
        {
            cout<<" ";
        }
        for(int k=m;k>=1;k--)
        {
            cout<<k;
        }
        cout<<endl;
}

    }
};
